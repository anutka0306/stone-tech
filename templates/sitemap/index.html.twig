{% extends 'v2/base.html.twig' %}

{% block body %}
    <div class="sitemap-container">
        <ul>
            {% for item in pagination %}
                <li><a href="{{ item.path }}">{{ item.h1 }}</a></li>
            {% endfor %}
        </ul>
        <div class="navigation">
            {{ knp_pagination_render(pagination) }}
        </div>
    </div>


{% endblock %}
